  <!-- Portfolio Grid -->
  <section class="bg-light" id="portfolio">
    <div class="container">
      <div class="row">
            <div class="container mt-4">
                    <div class="row d-flex justify-content-center">
                        <aside class="col-md-6">
                            <div class="card">
                                <article class="card-body">
                                    <h4 class="card-title text-center mb-4 mt-1">Actualizar usuario</h4>
                                    <hr>
                                    <form action="/update" method="POST" enctype="multipart/form-data">
                                        <input type="hidden" name="usuario_id" value='<%= usuario.usuario_id %>'>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text"> <i class="fa fa-user"></i> </span>
                                                </div>
                                                <input id="nombres" name="nombres" value="<%= usuario.nombres %>" class="form-control" placeholder="Nombres" required>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text"> <i class="fa fa-user"></i> </span>
                                                </div>
                                                <input id="apellidos" name="apellidos" value="<%= usuario.apellidos %>" class="form-control" placeholder="Apellidos" required>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text"> <i class="fas fa-envelope"></i> </span>
                                                </div>
                                                <input id="email" name="email" value="<%= usuario.email %>" class="form-control" placeholder="Email" type="email" required>
                                            </div>
                                        </div>
            
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text"> <i class="fa fa-lock"></i> </span>
                                                </div>
                                                <input id="nacimiento" name="nacimiento" value="<%= moment(usuario.nacimiento).format('YYYY-MM-DD'); %>" class="form-control" placeholder="22-10-1999" type="date" required>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text"> <i class="fas fa-file-image"></i></i> </span>
                                                </div>
                                                <input type="file" class="form-control" id="archivo" name="archivo">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <button type="submit" class="btn btn-primary btn-block">Actualizar</button>
                                        </div>
                                        <div class="form-group">
                                            <a href="/" class="btn btn-danger btn-block">Cancelar</a>
                                        </div>
                                    </form>
                                </article>
                            </div>
